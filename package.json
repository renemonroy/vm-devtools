{
  "name": "vm-devtools",
  "description": "Just a simple client to help code development for VM app.",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --colors --port 8081",
    "start": "NODE_ENV=development ./node_modules/.bin/electron .",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.4.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.36.5",
    "electron-rebuild": "^1.1.1",
    "eslint": "^2.0.0",
