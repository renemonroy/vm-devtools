    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "chokidar": "^1.4.2",
    "fs-plus": "^2.8.1",
    "immutable": "^3.7.6",
    "lodash": "^4.3.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-motion": "^0.3.1",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0",
    "redux": "^3.0.5",
    "redux-logger": "^2.5.0",
    "redux-saga": "^0.8.1",
    "trash": "^3.4.1"
