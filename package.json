{
  "name": "vm-devtools",
  "description": "Just a simple client to help code development for VM app.",
  "version": "0.1.0",
  "main": "main.js",
  "scripts": {
    "watch" : "NODE_ENV=development webpack-dev-server --colors --port 8081",
    "start": "NODE_ENV=development ./node_modules/.bin/electron .",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.36.5",
    "electron-rebuild": "^1.1.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "radium": "^0.16.4",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-motion": "^0.3.1",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-react-display-name",
      "transform-decorators-legacy"
    ]
  }
}
